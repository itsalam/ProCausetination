{
    "manifest_version": 2,
    "name" : "time-block thing",
    "version": "0.1",
    "default_locale": "en",
    "description": "Blocks tabs, and plays an ad that will go to a donation/person when you enter the site outside of your restrictions",
    "icons": {
      "128": "icon_16.png",
      "128": "icon_32.png",
      "128": "icon_48.png",
      "128": "icon_128.png"
    },
    "background": {
      "persistent": false,
      "scripts": ["background.js"]
    },
    "permissions": ["activeTab", "storage"],
    "browser_action": {
      "default_icon": "icon_16.png",
      "default_popup": "popup.html"
    }
}